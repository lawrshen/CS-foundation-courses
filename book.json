{
    "title": "CS-foundation-courses",
    "author": "lawrshen",
    "styles": {
        "website": "styles/website.css"
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-pro",
        "chapter-fold",
        "anchors",
        "github",
        "katex",
        "mermaid-gb3",
        "page-toc-button",
        "auto-scroll-table",
        "popup",
        "theme-comscore"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://lawrshen.github.io/CS-foundation-courses/"
        },
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first",
            "showByDefault": true
        }
    }
}